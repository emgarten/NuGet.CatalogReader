<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>NuGetMirror</id>
    <version>$version$</version>
    <authors>emgarten</authors>
    <owners>emgarten</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <licenseUrl>https://github.com/emgarten/nuget.catalogreader/blob/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/emgarten/nuget.catalogreader</projectUrl>
    <iconUrl>https://emgartenstatic.blob.core.windows.net/nupkgs/icons/nugetmirror.png</iconUrl>
    <description>NuGet v3 catalog reader.</description>
    <tags>nuget nugetcatalog nugetfeed nugetv3 mirror</tags>
    <repository type="git" url="https://github.com/emgarten/nuget.catalogreader" branch="$branch$" commit="$commit$" />
    <packageTypes>
      <packageType name="DotnetTool" />
    </packageTypes>
  </metadata>
  <files>
    <file src="..\..\artifacts\publish\*.exe" target="tools\" />
    <file src="..\..\artifacts\publish\netcoreapp2.1\*.dll" target="tools\netcoreapp2.1\any\" />
    <file src="..\..\artifacts\publish\netcoreapp2.1\DotnetToolSettings.xml" target="tools\netcoreapp2.1\any\" />
    <file src="..\..\artifacts\publish\netcoreapp2.1\NuGetMirror.runtimeconfig.json" target="tools\netcoreapp2.1\any\" />
    <file src="..\..\build\nupkgIncludes\NuGetMirror.props" target="build\NuGetMirror.props" />
  </files>
</package>
